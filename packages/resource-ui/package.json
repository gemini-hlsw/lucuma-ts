{
  "name": "resource-ui",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gemini-hlsw/navigate-ts.git",
    "directory": "packages/resource-ui"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:eslint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "devDependencies": {
    "lucuma-common-ui": "workspace:*",
    "lucuma-ui-css": "0.175.0",
    "tailwind-merge": "3.4.0"
  },
  "bundlemon": {
    "baseDir": "./dist",
    "pathLabels": {
      "hash": "[a-zA-Z0-9\\-_]+"
    },
    "files": [
      {
        "path": "assets/*-<hash>.{js,css}"
      }
    ],
    "reportOutput": [
      "github"
    ]
  }
}
